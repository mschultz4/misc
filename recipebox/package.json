{
  "name": "recipebox",
  "version": "1.0.0",
  "description": "recipe box project for codecamp",
  "main": "server.js",
  "scripts": {
    "start": "browserify -d -t [reactify] ./src/app.js --exclude react --exclude react-dom   > ./dist/bundle.js && node server.js",
    "build": "browserify -t [reactify] ./src/app.js --exclude react --exclude react-dom | uglifyjs -cm > ./dist/bundle.min.js",
    "watch": "watchify -d -t [reactify] ./src/app.js  -o ./dist/bundle.js",
    "clean": "esformatter -i './src/**/*.js'"
  },
  "author": "Matt Schultz",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "browser": "^0.2.6",
    "express": "^4.14.0",
    "flux": "^2.1.1",
    "keymirror": "^0.1.1",
    "lodash": "^4.14.1",
    "object-assign": "^4.1.0",
    "react": "^15.3.0",
    "react-bootstrap": "^0.30.2",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "eslint": "^3.2.2",
    "reactify": "^1.1.1",
    "uglify-js": "^2.7.0"
  }
}
